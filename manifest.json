{
  "name": "VRChatのWebで自動的にShow More Locationsを押すやつ",
  "version": "1.0.2",
  "description": "その名の通りの機能",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": [
        "https://vrchat.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "icons": {
    "128": "img/icon/icon128.png",
    "512": "img/icon/icon512.png"
  }
}
